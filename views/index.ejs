<html>
	<head>
		<link href="http://netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css" rel="stylesheet">	
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />	
	</head>
	<body>
		<nav class="navbar">
			
		</nav>
		<div class="container">
		<section id="section1" class="panel panel-default">

			<div class="panel-heading">
			<div class="container">
	    		<h3 class="panel-title">Please, select file to edit</h3>
	    		</div>
		  	</div>
	  		<div class="panel-body">
	  <form action="https://<%=bucket%>.s3.amazonaws.com/" method="POST" id="sign-up-form" class="form-horizontal" enctype="multipart/form-data">	
	
	<div class="container">
	<% fields.forEach(function(elem){ %>
	  <input type="hidden" name="<%= elem.name  %>" value="<%=elem.value %>"/>
		<%}); %> 
	   
	<div class="col-xs-5">
	<div class="form-group">
    	<label for="exampleInputFile">File input</label>
    	<input type="file" name="file">
    	<p class="help-block">File will be sent to the bucket <%=bucket%> and it's title will be sent to the SQS Que </p>
  </div>
 
	</div>
	</div>
	
	<div class="container">
	<div class="col-xs-3">
	<input type="submit" class="btn  btn-primary btn-block" value="Confirm"/>
	</div>
	</div>
</form>
</section>




<section id="section1" class="panel panel-default">
			<div class="panel-heading">
			<div class="container">
	    		<h3 class="panel-title">Lista plików nie przetworzonych</h3>
	    		</div>
		  	</div>
	  		<div class="panel-body">
				<div class="container">
					<div class="col-xs-5">
						<% links.forEach(function(elem){ %>
							<a href="https://s3-us-west-2.amazonaws.com/lab4-weeia/piotrwozniak/<%=elem.nazwa%>">     <%= elem.nazwa  %>   </a> |  
							<br>
						<% }) ;%> 
					</div>
				</div>
	  		</div>

			
</section>
		
		

<section id="section1" class="panel panel-default">
			<div class="panel-heading">
			<div class="container">
	    		<h3 class="panel-title">Lista plików przetworzonych</h3>
	    		</div>
		  	</div>
	  		<div class="panel-body">
				<div class="container">
					<div class="col-xs-5">
						<% linkiprzet.forEach(function(elem){ %>
							<a href="https://s3-us-west-2.amazonaws.com/lab4-weeia/processed/<%=elem.nazwa%>">     <%= elem.nazwa  %>   </a> |  
							<br>
						<% }) ;%> 
					</div>
				</div>
	  		</div>
 
	  		</div>
			<footer class="panel-footer">
				
			</footer>
</section>		
		
		
		
		
		
		
		
	</div>		
	</body>
</html>
